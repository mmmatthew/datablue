{
  "name": "datablue",
  "version": "1.0.0",
  "description": "datablue",
  "main": "index.js",
  "engines": {
    "node": "8.11.1"
  },
  "scripts": {
    "start": "cd build && node main",
    "init_symlink_server": "ln -sf /etc/letsencrypt/live/water-fountains.org/cert.pem certificate.pem && ln -sf /etc/letsencrypt/live/water-fountains.org/privkey.pem privatekey.pem",
    "compile": "node build.js && backpack build",
    "dev": "node build.js && backpack dev | pino -o 'short'",
    "debug": "node build.js && backpack dev --inspect-brk | pino -o 'short'",
    "test": "node build.js && _mocha --compilers js:babel-register --exit",
    "postinstall": "node build.js && backpack build",
    "heroku-prebuild": "cp .envTEMPLATE .env"
  },
  "dependencies": {
    "@turf/distance": "^6.0.1",
    "axios": "^0.18.0",
    "body-parser": "^1.18.2",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "dotenv": "^5.0.1",
    "express": "^4.16.2",
    "fs": "0.0.1-security",
    "helmet": "^3.12.1",
    "https": "^1.0.0",
    "lodash": "^4.17.10",
    "pino": "^4.15.0",
    "query-overpass": "^1.5.3",
    "source-map-support": "^0.5.4",
    "swagger-express-middleware": "^1.0.0-alpha.12",
    "wikidata-sdk": "^5.11.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-preset-env": "^1.6.1",
    "backpack-core": "^0.7.0",
    "chai": "^4.1.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.2",
    "eslint-plugin-import": "^2.7.0",
    "mocha": "^5.0.5",
    "shelljs": "^0.8.1",
    "supertest": "^3.0.0"
  },
  "author": "Carmine DiMascio <cdimascio@gmail.com> (https://github.com/cdimascio)"
}
